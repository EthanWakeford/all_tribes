---
import Layout from '../layouts/Layout.astro';
import Main from '../components/Main';
import Gallery from '../components/Gallery';

const img1 = 'src/assets/all_tribes_property.jpg';
const img2 = 'src/assets/story_night_crowd.jpg';
const img3 = 'src/assets/story_night_speaker.jpg';
---

<Layout>
  <Main />
  <Gallery
    images={[
      { src: img1, alt: 'alt' },
      { src: img2, alt: 'alt' },
      { src: img3, alt: 'alt' },
    ]}
    client:visible
  />
</Layout>
